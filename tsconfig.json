{
  "compilerOptions": {
    "baseUrl": "./src/",
    "outDir": "./dist/",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["es6", "dom", "es2016", "es2017"],
    "inlineSourceMap": true,
    "inlineSources": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "moduleResolution": "node",
    "strict": false,
    "jsx": "react",
    "declarationMap": true,
    "esModuleInterop": true
    },
  "exclude": ["node_modules"]
}